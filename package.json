{
  "name": "react-git-provider",
  "version": "0.0.1",
  "description": "A Git provider for React",
  "author": {
    "name": "David Baumgartner",
    "email": "dv@bmgr.me",
    "url": "https://dv.bmgr.me"
  },
  "license": " GPL-3.0-or-later",
  "homepage": "https://react-git-provider.publica.re/",
  "bugs": {
    "url": "https://github.com/publica-re/react-git-provider/issues",
    "email": "dv@bmgr.me"
  },
  "repository": "publica-re/react-git-provider",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "start-example": "cd example && yarn install && yarn start",
    "build-example": "cd example && yarn install && yarn build",
    "docs": "typedoc --out docs src && yarn build-example && cp -R example/build docs/demo",
    "dev-serve-docs": "yarn docs && cd docs && python -m http.server"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "eslint": "^6.8.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@isomorphic-git/lightning-fs": "^4.3.0",
    "@material-ui/icons": "^4.9.1",
    "@types/diff": "^4.0.2",
    "@types/lodash": "^4.14.159",
    "bind-decorator": "^1.0.11",
    "diff": "^4.0.2",
    "eventemitter3": "^4.0.4",
    "isomorphic-git": "^1.7.4",
    "jsdiff": "^1.1.1",
    "lodash": "^4.17.19",
    "path": "^0.12.7"
  }
}
